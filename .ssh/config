    TCPKeepAlive=yes
    ServerAliveInterval=10
    ServerAliveCountMax=4
    Compression=yes
    ControlMaster auto
    ControlPath /tmp/%r@%h:%p
    ControlPersist 3600
    SendEnv LANG=C ITERM_PROFILE
    ForwardAgent yes

